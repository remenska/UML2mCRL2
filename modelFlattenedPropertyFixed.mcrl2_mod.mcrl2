eqn
    mapSelectExpired([]) = [];
    (PinExpired(el)) -> mapSelectExpired(el|>listStageRequests) = ReplicaID(el)|>mapSelectExpired(listStageRequests);
    !(PinExpired(el)) -> mapSelectExpired(el|>listStageRequests) = mapSelectExpired(listStageRequests);
